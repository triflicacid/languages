<?xml version="1.0" encoding="UTF-8"?>
<!-- - Use this code to load the XML stylesheet:
- <?xml-stylesheet type="text/xsl" href="style.xsl"?> -->
<page>
	<title>Document Title</title>
	<doclinks>
		<!-- Document LInks to other pages -->
		<link> <!-- Each link will be surrounded by <link> -->
			<url>URL to go to</url>
			<text>Link text to display</text>
		</link>
	</doclinks>
	
	<line>
		<!-- Each line will be surrounded by <line> -->
		<number>Line Number</number>
		<note>Urgent line comment</note>
		<latin>Latin Text</latin>
		<english>English translation</english>
		<notes>
			<title>Header for notes</title>
			<!-- Contains line notes -->
			<point>
				<!-- Starts a bullet point. To start a new bullet point, add another <point> -->
				<main>Focus of point</main>
				<text>Explanation</text>
			</point>
		</notes>
	</line>
	
</page>